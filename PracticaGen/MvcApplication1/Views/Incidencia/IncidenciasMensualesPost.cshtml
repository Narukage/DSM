@using System.Data
@model DataTable

@{
    ViewBag.Title = "IncidenciasMensualesPost";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<a id="back" href='@Url.Action("PanelAdmin", "Home")'><img id="atras" src="~/Content/themes/base/images/atras.png" /></a>

<h2>Estadística de las incidencias mensuales</h2>

<table border="1" cellpadding="5">
    <thead>
        <tr>
            @foreach (System.Data.DataColumn col in Model.Columns)
            {
                <th>@col.Caption</th>
            }
        </tr>
    </thead>
    <tbody>
        @foreach (System.Data.DataRow row in Model.Rows)
        {
            <tr>
                @foreach (var cell in row.ItemArray)
                {
                    <td>@cell.ToString()</td>
                }
            </tr>
        }
    </tbody>
</table>