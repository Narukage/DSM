@model MvcApplication1.Models.Pedido

@{
    ViewBag.Title = "Delete";
}

<h2>Delete</h2>

<h3>Estas seguro de que deseas borrar este pedido?</h3>
<fieldset>
    <legend>Codigo</legend>

    <div class="display-label">
         @Html.DisplayNameFor(model => model.TipoPago)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Fecha)
    </div>

    <div class="display-label">
         @Html.DisplayNameFor(model => model.iduser)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.TipoPago)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Estado)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Confirmado)
    </div>

</fieldset>
@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    <p>
        <input type="submit" value="Eliminar" /> |
        @Html.ActionLink("Volver a la lista", "Index")
    </p>
}
